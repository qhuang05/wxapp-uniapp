{
	"pages": [
		{
			"path": "pages/tabBar/design/index",
			"style": {
				"navigationBarTitleText": "定制首页",
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white",
				"onReachBottomDistance": 120
			},
			"meta": {
				"auth": false
			}
		},
		{
			"path": "pages/tabBar/cart/index",
			"style": {
				"navigationBarTitleText": "购物车"
			},
			"meta": {
				"auth": true
			}
		},
		{
			"path": "pages/tabBar/user/index",
			"style": {
				"navigationBarTitleText": "功能列表"
			},
			"meta": {
				"auth": true
			}
		},
		{
			"path": "pages/login/index",
			"style": {
				"navigationBarTitleText": "登录"
			},
			"meta": {
				"auth": false
			}
		}
	],
	"subPackages": [{
		"root": "pages/moduleA",
		"pages": [
			{
				"path": "address/index",
				"style": {
					"navigationBarTitleText": "地址管理"
				},
				"meta": {
					"auth": true
				}
			},
			{
				"path": "address/edit",
				"style": {
					"navigationBarTitleText": "地址编辑"
				},
				"meta": {
					"auth": true
				}
			},
			{
				"path": "pay/index",
				"style": {
					"navigationBarTitleText": "支付"
				},
				"meta": {
					"auth": true
				}
			},
			{
				"path": "share/index",
				"style": {
					"navigationBarTitleText": "分享"
				},
				"meta": {
					"auth": true
				}
			},
			{
				"path": "upload/index",
				"style": {
					"navigationBarTitleText": "上传下载"
				},
				"meta": {
					"auth": true
				}
			}
		]
	},{
		"root": "pages/moduleB",
		"pages": [
			{
				"path": "shipping/index",
				"style": {
					"navigationBarTitleText": "物流管理"
				},
				"meta": {
					"auth": true
				}
			}
		]
	}],
	"globalStyle": {
		"navigationBarBackgroundColor": "#000",
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"backgroundColor": "#000"
	},
	"tabBar": {
	    "backgroundColor": "#fff",
	    "color": "#000",
	    "selectedColor": "#FF4D4F",
	    "borderStyle": "white",
	    "list": [
			{
				"pagePath": "pages/tabBar/cart/index",
				"iconPath": "static/tabbars/cart.png",
				"selectedIconPath": "static/tabbars/cart-selected.png",
				"text": "购物车"
			},
			{
				"pagePath": "pages/tabBar/design/index",
				"iconPath": "static/tabbars/home.png",
				"selectedIconPath": "static/tabbars/home-selected.png",
				"text": "定制"
			},
			{
				"pagePath": "pages/tabBar/user/index",
				"iconPath": "static/tabbars/user.png",
				"selectedIconPath": "static/tabbars/user-selected.png",
				"text": "个人中心"
			}
		]
	},
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	//模式配置，仅开发期间生效
	"condition": { 
	    "current": 0,
	    "list": [
			{
	            "name": "uploadIndex", //模式名称
	            "path": "pages/moduleA/upload/index", //启动页面，必选
	            "query": "" //启动参数，在页面的onLoad函数里面得到。
	        },
			{
			    "name": "userIndex",
			    "path": "pages/tabBar/user/index",
			    "query": "interval=4000&autoplay=false"
			}
	    ]
	}
}
